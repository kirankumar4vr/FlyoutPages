<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:FlyoutPagesDemo.Views"
             x:Class="FlyoutPagesDemo.Views.LandingFlyoutPage"
             Title="LandingFlyoutPage">
    <FlyoutPage.Flyout>
        <views:HamburgerMenuPage x:Name="HamburgerMenuPage"/>
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage
            BarBackgroundColor="Blue"
            BarTextColor="White">
            <x:Arguments>
                <views:LandingPage/>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>
